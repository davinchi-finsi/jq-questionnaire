mixin jq-questionnaire__result(params)
    //Questionnaire result
    - params = params || {};
    +_mod-baseContent({
        internalParams: {
            cssClass: "jq-questionnaire__result",
            attributes: {
                "data-jq-questionnaire-result": ""
            }
        },
        externalParams: params
    })&attributes(attributes)
        if(params.items)
            each item in params.items
                +jq-questionnaire__result-item(item)
        block
    //End questionnaire result